<p class="outlet_sub_title">Hosts</p>
<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Status</th>
            <th scope="col">Hostname</th>
            <th scope="col">CPU</th>
            <th scope="col">IP</th>
            <th scope="col">Created</th>
        </tr>
    </thead>
    <tbody>
        {{#each model.hosts as |host|}}
        <tr>
            <td><span class="cluster-status vertical-middle status-active">{{host.state}}</span></td>
            <td>{{host.hostname}}</td>
            <td>{{host.info.cpuInfo.modelName}}</td>
            <td>{{host.agentIpAddress}}</td>
            <td>{{host.created}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>